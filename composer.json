{
    "name": "afbora/kirby-widgets",
    "description": "Panel widgets for Kirby 3",
    "keywords": [
        "kirby",
        "kirby-3",
        "kirby3-plugin",
        "kirby-plugin",
        "kirby-widgets",
        "widgets"
    ],
    "version": "1.0.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet Bora",
            "email": "ahmet.bora@pixelturk.net"
        }
    ],
    "require": {
        "php": "^7.2",
        "getkirby/composer-installer": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
